{
  "name": "hikrc-menu",
  "version": "0.0.1",
  "description": "menu ui component for react",
  "keywords": [
    "react",
    "hic-react-component",
    "menu",
    "ui",
    "hik-react-menu"
  ],
  "files": [
    "lib",
    "assets/*.css"
  ],
  "main": "./lib/index",
  "homepage": "http://github.com/hic-react-component/menu",
  "maintainers": [
    "chenpi@cetiti.com"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:hic-react-component/menu.git"
  },
  "bugs": {
    "url": "http://github.com/hic-react-component/menu/issues"
  },
  "licenses": "MIT",
  "config": {
    "port": 8010
  },
  "scripts": {
    "build": "rc-tools run build",
    "gh-pages": "rc-tools run gh-pages",
    "start": "rc-tools run server",
    "pub": "rc-tools run pub --babel-runtime",
    "lint": "rc-tools run lint",
    "karma": "rc-tools run karma",
    "saucelabs": "rc-tools run saucelabs",
    "test": "jest",
    "chrome-test": "rc-tools run chrome-test",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-antd": "^0.5.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^2.11.15",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.1",
    "expect.js": "~0.3.1",
    "jest": "^17.0.3",
    "pre-commit": "1.x",
    "rc-tools": "5.x",
    "react": "15.x",
    "react-addons-test-utils": "15.x",
    "react-dom": "15.x"
  },
  "dependencies": {
    "babel-runtime": "6.x",
	"css-animation": "^1.2.0",
    "classnames": "2.x",
    "dom-scroll-into-view": "1.x",
    "rc-animate": "2.x",
    "rc-util": "3.x",
	"warning": "~3.0.0"
  },
  "pre-commit": [
    "lint"
  ]
}
